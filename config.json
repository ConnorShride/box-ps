    "Cmdlets": {
        "file_system": {
            "Microsoft.PowerShell.Management\\Get-Item": {
                "BehaviorPropArgs": {
                    "paths": ["Path", "LiteralPath"]
                },
                "Flags": ["call_parent"]
            },
            "Microsoft.PowerShell.Management\\Set-Content": {
                "BehaviorPropArgs": {
                    "paths": ["Path", "LiteralPath"]
                }
            },
            "Microsoft.PowerShell.Management\\Set-Location": {
                "BehaviorPropArgs": {
